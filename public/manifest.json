{
  "manifest_version": 3,
  "name": "Kinematic Character Controller Example",
  "description": "This example shows how to create a Kinematic Character Controller using Rapier.js and Three.js",
  "version": "0.0.1",
  "action": {
    "default_popup": "index.html",
    "default_title": "Kinematic Character Controller Example"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'"
  },
  "icons": { 
    "16": "png/icon16.png",
    "32": "png/icon32.png",
    "48": "png/icon48.png",
    "128": "png/icon128.png"
  }
}